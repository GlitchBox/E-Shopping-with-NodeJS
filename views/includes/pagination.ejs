<section class="pagination">
    <%if(currentPage !== 1){%>
        <a href="<%= path %>?page=<%= prevPage %>">Previous</a>
        <!-- <a href="/?page=1">1</a> -->
    <%}%>
    <a href="<%= path %>?page=<%= currentPage %>" class="active"><%= currentPage %></a>
    <% if(hasNextPage){%>
        <!-- <a href="/?page=<%= lastPage %>"><%= lastPage %></a> -->
        <a href="<%= path %>?page=<%= nextPage %>">Next</a>
    <%}%>
</section>